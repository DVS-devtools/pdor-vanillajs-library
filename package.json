{
  "name": "",
  "scripts": {
    "build": "rimraf ./dist && cross-env NODE_ENV=production webpack",
    "doc": "rimraf ./docs/${npm_package_version} && jsdoc -c .jsdoc.json -d ./docs/${npm_package_version}/",
    "doc:latest": "rimraf ./docs/latest && jsdoc -c .jsdoc.json -d ./docs/latest/",
    "doc:open": "npm run doc && open docs/${npm_package_version}/index.html",
    "lint": "eslint \"src/**\"",
    "start": "cross-env NODE_ENV=development webpack-dev-server --open",
    "test": "jest --config jest.config.json",
    "test:watch": "npm run test -- --watchAll",
    "test:open": "npm run test && open coverage/lcov-report/index.html",
    "test:coveralls": "npm run test -- --coverageReporters=text-lcov | coveralls",
    "preversion": "npm run lint && npm run test",
    "version": "npm run build && npm run doc && npm run doc:latest",
    "postversion": "git add docs/ && git commit -am \"Documentation ${npm_package_version}\" && git push --follow-tags"
  },
  "main": "dist/index.js",
  "license": "MIT",
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "7.1.2",
    "@babel/plugin-proposal-class-properties": "7.1.0",
    "@babel/plugin-proposal-decorators": "7.1.2",
    "@babel/plugin-proposal-export-namespace-from": "7.0.0",
    "@babel/plugin-syntax-dynamic-import": "7.0.0",
    "@babel/plugin-transform-runtime": "7.1.0",
    "@babel/polyfill": "7.0.0",
    "@babel/preset-env": "7.1.0",
    "@babel/register": "7.0.0",
    "@babel/runtime": "7.1.2",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "10.*.*",
    "babel-loader": "8.0.4",
    "coveralls": "3.*.*",
    "cross-env": "^5.2.0",
    "css-loader": "^2.0.0",
    "eslint": "5.*.*",
    "eslint-config-airbnb-base": "13.*.*",
    "eslint-loader": "^2.1.1",
    "eslint-plugin-import": "2.*.*",
    "file-loader": "^2.0.0",
    "html-webpack-plugin": "^3.2.0",
    "jest": "23.*.*",
    "jsdoc": "3.*.*",
    "minami": "1.*.*",
    "mini-css-extract-plugin": "^0.4.4",
    "rimraf": "^2.6.2",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "url-loader": "^1.1.2",
    "webpack": "^4.20.2",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.9",
    "webpack-merge": "^4.1.4"
  },
  "pdor": {
    "type": "Vanilla Js",
    "renameOptions": {
      "replaceInFiles": [
        {
          "stubName": "YourLibrary",
          "to": ":targetName",
          "files": [
            "src/index.js",
            "src/init/index.js",
            "src/init/init.test.js",
            "example/index.js",
            "example/index.html",
            "webpack/webpack.config.js"
          ]
        },
        {
          "stubName": "LogLibrary",
          "to": ":targetName",
          "files": [
            "src/log/index.js"
          ]
        }
      ]
    }
  }
}
